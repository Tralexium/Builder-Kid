// Customize these if you want
speedSign = 1 // 1 or -1, keeps track of which way to move next
dist = 32     // how far to move
moveSpeed = 6 // how fast to move

xScalar = 0 // 1 to move right first, -1 to move left first, 0 to not move horizontally
yScalar = 0 // 1 to move down first, -1 to move up first, 0 to not move veritcally

count = 0  // from 0 to 32, how far we've moved
dCount = 0 // delta count: how much to move each step, or 0 to not move

